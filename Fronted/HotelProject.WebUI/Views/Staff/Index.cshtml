@model List<StaffViewModel>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<table class="table table-bordered table-hover">
    <tr>
        <th>#</th>
        <th>personel ad</th>
        <th>personel ünvan</th>
        <th>sil</th>
       <th>güncelle</th>
        <th>detaylar</th>
    </tr>
    
        @foreach (var item in Model)
        {
<tr>
    <td>@item.ID</td>
    <td>@item.Name</td>
    <td>@item.Title</td>

    <td><a href="/Staff/DeleteStaff/@item.ID" class="btn mb-1 btn-rounded btn-outline-danger">Sil</a></td>
    <td><a href="/Staff/UpdateStaff/@item.ID" class="btn mb-1 btn-rounded btn-outline-success">Güncelle</a></td>
    <td><a href="#" class="btn mb-1 btn-rounded btn-outline-dark">Detaylar</a></td>
</tr>
        }
  
  

</table>
<a asp-action="AddStaff" asp-controller="Staff" class=" btn mb-1 btn-rounded btn-outline-info">yeni personel girişi</a>

